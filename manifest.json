{
    "manifest_version": 2,
    "name": "Voice-to-text",
    "version": "0.0.1",
    "description": "Convert voice into text for.",
    "homepage_url": "https://github.com/ankitkhushwaha/voice-to-text/",
    "icons": {
        "48": "icons/border-48.png"
    },
    "content_scripts": [
        {
            "matches": [
                "<all_urls>"
            ],
            "js": [
                "app.js"
            ]
        }
    ],
    "browser_action": {
        "default_icon": "icons/border-48.png",
        "default_title": "Voice-to-text"
    },
    "browser_specific_settings": {
        "gecko": {
            "id": "borderify@example.com"
        }
    }
}